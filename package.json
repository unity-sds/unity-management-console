{
	"name": "svelteapp-typescript-go",
	"version": "0.1.0",
	"license": "MIT",
	"private": true,
	"scripts": {
		"setup": "npm i && npm prune",
		"setup-all": "npm run setup && echo \"Download Golang dependencies...\" && go get ./... && go mod tidy",
		"upgrade": "npx npm-check-updates -u && npm run setup",
		"upgrade-all": "npx npm-check-updates -u && npm run setup && echo \"Update Golang dependencies...\" && go get -u ./...",
		"dev": "vite dev --open",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"build": "npm run check && npx vite build --base=/ui/",
		"build-all": "npm run build && echo \"Building Golang binary...\" && go build -buildvcs=false -o ./main ./backend/cmd/web",
		"serve": "open-cli http://localhost:8080 && ./main",
		"serve-win": "open-cli http://localhost:8080 && main",
		"docker": "docker build . -t svelte-ts-go -f Dockerfile",
		"docker-run": "open-cli http://localhost:8080 && docker run -p 8080:8080 --rm svelte-ts-go",
		"test": " go test -buildvcs=false ./backend/...",
		"commit": "npm run format && git pull origin main && git add . && git commit -m \"Updating\" && git push origin main",
		"package": "npm run build-all && rm -rf management-console && mkdir management-console &&  cp -rf build static main workflowresources management-console && zip -r managementconsole.zip management-console",
		"build-integration": "go build -buildvcs=false -o ./integration ./backend/cmd/integration && zip -r integration.zip ./integration",
		"build-proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/data/  ./unity-cs-manager/protobuf/extensions.proto ./unity-cs-manager/protobuf/marketplace.proto && protoc --proto_path=protobuf --go_out=backend/internal/models/ --go_opt=paths=source_relative websocket.proto"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.16.3",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.5",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-check-updates": "^16.10.11",
		"open-cli": "^7.2.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.62.1",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.5"
	},
	"type": "module",
	"dependencies": {
		"@nasa-jpl/stellar": "^1.1.4",
		"atob": "^2.1.2",
		"axios": "^1.4.0",
		"bootstrap": "^5.2.3",
		"svelte-resize-observer": "^2.0.0",
		"ts-proto": "^1.149.0",
		"ws": "^8.13.0",
		"xterm": "^5.2.1",
		"xterm-addon-fit": "^0.7.0"
	}
}
